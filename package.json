{
  "name": "tres-connect",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fontsource/roboto": "^4.5.7",
    "@mui/material": "^5.9.3",
    "@types/node": "^16.11.47",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "craco": "^0.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^4.0.0",
    "typescript": "^4.7.4",
    "watch": "^0.13.0"
  },
  "scripts": {
    "build": "INLINE_RUNTIME_CHUNK=false craco build",
    "watch": "INLINE_RUNTIME_CHUNK=false watch 'craco build' ./src ./public",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx,md,json}'",
    "lint:fix": "eslint './src/**/*.{js,jsx,ts,tsx,md,json}' --fix",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "devDependencies": {
    "@types/chrome": "^0.0.193",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1"
  }
}
